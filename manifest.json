{
  "manifest_version": 2,

  "name": "Save_tabs",
  "description": "This extension saves all the links opened at present in the window",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Save_now!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}